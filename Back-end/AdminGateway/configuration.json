        {
          "Host": "localhost",
          "Port": 44327
        }
      ],
      "UpstreamPathTemplate": "/api/{entity}/config",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/storage/{entity}/form",
      "DownstreamScheme": "https",
